--buyers texts
x2price = 100 -- settin variables
cpsprice = 50
local surfacegui = Instance.new("SurfaceGui",x2part)
local textlabel = Instance.new("TextLabel",surfacegui)
surfacegui.Face = "Left"
surfacegui.CanvasSize = Vector2.new(600, 600)
