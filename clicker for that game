textlabel.Text = "+1 Click/Sec"..[[

]]..cpsprice
cpsparttl = textlabel
--buyers texts

buttoncps = 0
coroutine.wrap(function() -- starting a loop inside coroutine wrap
while true do
    wait(1)
    buttonclicks = buttonclicks + buttoncps
    buttontl.Text = buttonclicks
end
end)()

buttonclicks = 0
buttonclick = 1
local Player = game.Players.LocalPlayer --getting localplayer
local mouse = Player:GetMouse() -- gettin mouse
mouse.Button1Down:Connect(function() --when clicked
if mouse.Target == button then --if cursor is looking at da button
    buttonclicks = buttonclicks + buttonclick
    buttontl.Text = buttonclicks
    button.CFrame = CFrame.new(58.5, 4.14, 33.5)
    wait(0.1)
    button.CFrame = CFrame.new(58.5, 4.39, 33.5)
end
if mouse.Target == x2part then
    if buttonclicks >= x2price then -- stupid checks
        if buttonclicks < 20000 then
        cashsound:Play()
        x2price = x2price + (x2price * math.random(1,7) / 10)
        print(1)
        else
        x2price = x2price + (x2price * math.random(1,150) / 1000)
        print(2)
        end
        x2price = math.floor(x2price)
        buttonclick = buttonclick + 1
x2parttl.Text = "X2 Click"..[[

]]..x2price
    end
end

if mouse.Target == cpspart then
    if buttonclicks >= cpsprice then
        buttoncps = buttoncps + 1
        cashsound:Play()
        if cpsprice < 20000 then
        cpsprice = cpsprice + (cpsprice * math.random(1,6) / 10)
        print(10)
        else
        cpsprice = cpsprice + (cpsprice * math.random(1,150) / 1000)
        print(20)
        end
        cpsprice = math.floor(cpsprice)
        cpsparttl.Text = "+1 Click/Sec"..[[

]]..cpsprice
    end
end
end)
--da end
